    "@tsconfig/strictest": "^2.0.2",
    "@types/chrome": "0.0.251",
    "@types/node": "20.9.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "@vitejs/plugin-react": "^4.2.1",
    "jsdom": "^23.0.1",
    "typescript": "5.2.2",
    "vite": "^5.0.7",
    "vite-tsconfig-paths": "^4.2.2",
